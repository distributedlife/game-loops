{
  "name": "game-loops",
  "version": "1.0.0",
  "description": "Several kinds of game loop",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register -c -S -R progress src/**/*spec*.js",
    "predist": "rm -rf dist",
