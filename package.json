    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "require-inject": "^1.4.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "author": "Ryan Boucher",
  "license": "BSD-2-Clause",
  "dependencies": {
    "moment": "^2.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distributedlife/game-loops.git"
  },
